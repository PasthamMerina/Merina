#include <stdio.h>

int main() {
    int even_count = 0;

    // Loop through numbers from 1 to 500
    for (int i = 1; i <= 500; i++) {
        if (i % 2 == 0&& i%7==0) {
            even_count++;
        }
    }

    // Print the result
    printf("Count of even numbers from 1 to 500: %d\n", even_count);

    return 0;
}
